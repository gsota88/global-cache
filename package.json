{
  "name": "global-cache-monorepo",
  "private": true,
  "version": "0.2.5",
  "packageManager": "npm@11.6.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitalets/global-cache.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "tsc": "npm run build && tsc",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:w": "prettier --write --ignore-unknown .",
    "pw:install": "PLAYWRIGHT_SKIP_BROWSER_GC=1 npx playwright install chromium",
    "toc": "remark README.md --frail -o",
    "release": "release-it",
    "dev": "turbo watch build",
    "build": "turbo run build",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.54.2",
    "@release-it/bumper": "^7.0.5",
    "@release-it/keep-a-changelog": "^7.0.0",
    "@types/node": "^20.19.19",
    "eslint": "^9.37.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-visual-complexity": "0.1.4",
    "globals": "^16.3.0",
    "knip": "^5.62.0",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "publint": "^0.3.7",
    "release-it": "^19.0.4",
    "remark-cli": "^12.0.1",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "remark-validate-links": "^13.1.0",
    "turbo": "latest",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  }
}

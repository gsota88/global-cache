{
  "name": "global-cache-monorepo",
  "private": true,
  "packageManager": "npm@11.6.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitalets/global-cache.git"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "tsc": "tsc",
    "knip": "knip -c knip.config.ts",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:w": "prettier --write --ignore-unknown .",
    "pw": "npm i --no-save @playwright/test@$PW",
    "pw:install": "PLAYWRIGHT_SKIP_BROWSER_GC=1 npx playwright install chromium",
    "toc": "md-magic --files README.md",
    "release": "release-it",
    "dev": "turbo watch build",
    "build": "turbo run build",
    "test": "turbo run test --filter=./packages/*"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.54.2",
    "@release-it/keep-a-changelog": "^7.0.0",
    "@types/node": "^20.19.19",
    "eslint": "^9.37.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-visual-complexity": "0.1.4",
    "globals": "^16.3.0",
    "knip": "^5.62.0",
    "lint-staged": "^16.1.5",
    "markdown-magic": "^3.4.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.7",
    "release-it": "^19.0.4",
    "turbo": "latest",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  }
}
